{
  "name": "ew-color-picker",
  "version": "2.0.0",
  "description": "一个基于typescript封装的颜色选择器",
  "scripts": {
    "build": "node scripts/build.js",
    "tsc": "tsc",
    "build:build": "rollup -c",
    "build:docs": "node scripts/build-docs.js & cd docs && npm run build",
    "start:docs": "cd docs && npm run start",
    "dev": "rollup -c -w",
    "test": "ts-node test/test.ts",
    "pretest": "npm run build",
    "publish": "npm publish .",
    "publicPublish": "npm publish . --access public",
    "deploy": "npm run build:docs & gh-pages -d gh-pages",
    "release-docs": "gh-pages -d gh-pages"
  },
  "main": "dist/ew-color-picker.min.js",
  "module": "dist/ew-color-picker.esm.min.js",
  "typings": "./typings/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eveningwater/ew-color-picker.git"
  },
  "keywords": [
    "ew-color-picker"
  ],
  "author": "eveningwater",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eveningwater/ew-color-picker/issues"
  },
  "homepage": "https://github.com/eveningwater/ew-color-picker#readme",
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@rollup/plugin-alias": "^4.0.4",
    "@rollup/plugin-replace": "^5.0.5",
    "@types/ms": "^0.7.34",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-latest": "^6.24.1",
    "gh-pages": "^6.1.1",
    "node-sass": "^9.0.0",
    "rollup": "^2.79.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "terser": "^5.31.0",
    "ts-loader": "^8.4.0",
    "ts-node": "^9.1.1",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5",
    "@ew-color-picker/utils": "workspace:*"
  }
}
